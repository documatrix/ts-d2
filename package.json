{
  "name": "ts-d2",
  "version": "0.0.12",
  "description": "Deuterium is a TypeScript library for professional output creation.",
  "keywords": [
    "pdf",
    "postscript",
    "output",
    "formatting",
    "rendering",
    "typescript",
    "document"
  ],
  "homepage": "https://d2lib.io",
  "bugs": {
    "url": "https://github.com/documatrix/ts-d2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/documatrix/ts-d2.git"
  },
  "license": "MIT",
  "type": "module",
  "author": "DocuMatrix GmbH",
  "engines": {
    "node": ">=18.17.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "dprint fmt",
    "lint": "eslint src --ext .ts",
    "prepublish": "pnpm run build",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format && pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "dprint": "^0.51.1",
    "eslint": "9.22.0",
    "husky": "^9.1.7",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "docframe-types": "^0.7.4",
    "dotenv": "^17.3.1",
    "form-data": "^4.0.5",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0"
  }
}