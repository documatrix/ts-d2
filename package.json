{
  "name": "ts-d2",
  "version": "0.0.4",
  "description": "Deuterium is a TypeScript library for professional output creation.",
  "keywords": [
    "pdf",
    "postscript",
    "output",
    "formatting",
    "rendering",
    "typescript",
    "document"
  ],
  "homepage": "https://d2lib.io",
  "bugs": {
    "url": "https://github.com/documatrix/ts-d2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/documatrix/ts-d2.git"
  },
  "license": "MIT",
  "author": "DocuMatrix GmbH",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup-node",
    "build:dev": " cross-env NODE_ENV=development tsup-node",
    "format": "dprint fmt",
    "lint": "eslint src --ext .ts",
    "prepare": "husky",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format && pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@jest/globals": "^29.5.0",
    "@swc/core": "^1.11.9",
    "@swc/helpers": "^0.5.15",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.10",
    "@types/uuid": "^10.0.0",
    "typescript-eslint": "8.26.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "dprint": "^0.49.0",
    "esbuild": "0.25.1",
    "eslint": "9.22.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "semantic-release": "24.2.3",
    "ts-jest": "^29.2.6",
    "tslib": "2.8.1",
    "tsup": "8.4.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "docframe-types": "^0.4.2",
    "form-data": "^4.0.2",
    "uuid": "^11.1.0"
  }
}